{
  "name": "tuny",
  "version": "0.1.0",
  "description": "Tuner web app",
  "main": "index.js",
  "scripts": {
    "start": "run-s build:prod serve",
    "serve": "http-server .",
    "build:prod": "browserify src/js/app.js -t babelify --outfile public/dist/bundle.js  ",
    "build:dev": "browserify src/js/app.js -t babelify --debug | exorcist public/dist/bundle.js.map > public/dist/bundle.js ",
    "build:watch": "nodemon -e js --watch src --delay 1 --exec 'npm run build:dev'",
    "dev": "run-p build:watch serve",
    "test:dev": "nodemon -e js --watch test --watch src --delay 1 --exec 'npm run test'",
    "test:cov": "nyc npm test",
    "test": "tape -r @babel/register test/**/test.js test/**/*.js  ",
    "lint": "eslint index.js src/**/*.js tests/**/test.js"
  },
  "author": "Bertrand Chevrier <chevrier.bertrand@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cycle/dom": "^20.4.0",
    "@cycle/isolate": "^3.4.0",
    "@cycle/run": "^4.4.0",
    "xstream": "^11.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@cycle/time": "^0.18.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "eslint": "^5.1.0",
    "exorcist": "^1.0.1",
    "faucet": "0.0.1",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "nyc": "^13.1.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.1"
  }
}
